<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane minHeight="0.0" minWidth="0.0"
            prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.swe2.cs.views.EXIFView">
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0"
              AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <Label text="Camera"/>
                <TextField editable="false"
                           promptText="Which camera did you use?" fx:id="cameraTextField"/>
                <Label text="Lens">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Label>
                <TextField editable="false"
                           promptText="Which lens did you use?" fx:id="lensTextField"/>
                <Label text="Date">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Label>
                <TextField editable="false"
                           promptText="Choose date" fx:id="dateField"/>
                <Button visible="false"
                        alignment="CENTER"
                        contentDisplay="CENTER"
                        maxWidth="1.7976931348623157E308"
                        mnemonicParsing="false"
                        prefHeight="25.0"
                        text="Save"
                        textAlignment="CENTER">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Button>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0"
                        right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
